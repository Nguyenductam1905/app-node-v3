{
 "compilerOptions": {
   "target": "ES2020",
   "module": "commonjs",
   "outDir": "dist",
   "rootDir": "src",
   "strict": true,
   "esModuleInterop": true,
   "skipLibCheck": true,
   "forceConsistentCasingInFileNames": true,
   "sourceMap": true,
   "baseUrl": ".",
   "paths": {
     "services/*": ["./src/services/*"],
     "controllers/*": ["./src/controllers/*"],
     "models/*": ["./src/models/*"],
     "routes/*": ["./src/routes/*"],
     "config/*": ["./src/config/*"]
   },
 }
}
